<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="utf-8">
    <title>Bootshop online Shopping cart</title>
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="description" content="">
    <meta name="author" content="">

<%include head%>

    <style type="text/css">
    	.text{
			 
  color: white;
  margin: 20px;
  padding: 10px;
		}
    </style>>


  </head>
<body>
<%include header%>
<!-- Header End====================================================================== -->
<div id="mainBody">
	<div class="container">
	<div class="row">
<!-- Sidebar ================================================== -->


<!-- Sidebar end=============================================== -->
<div class="span9">

	<h1 class="text"><%=data.DisplayName%> </h1 >
    
	<img style="margin-left: 30px" src="<%=data.ImageAvatar%>" height="600px" width="500px">

	<h3 class="text">Nhà cung cấp sản phẩm : <%=data.Supplier%></h3>
	<h3 class="text">Giá sản phẩm : <%=data.Price%>đ</h3>
	<p></p>
	
	<% data.ImageInfo.forEach(function(d,i){ %>
	<img style="padding: 20px;" src="<%=d%>" height="460px" width="300px">
	
  	<% }); %>	

  	<div >
		<h5 class="text"><i class="fa icon-key-minus"></i>Thiết kế: <%=data.Info.thietke%></h5>
	</div>
	<div class="text">
		<h5><i class="fa icon-key-minus"></i>Hiển thi: <%=data.Info.HienThi%></h5>
	</div>
	<div >
		<h5 class="text">Cấu hình:</h5>
		<% data.Info.CauHInh.forEach(function(cauhinh,i){ %>
			<h5 class="text"><%=cauhinh%> </h5>
	  	<% }); %>	
	</div>

	<div >
		<h5 class="text">Khác:</h5>
		<% data.Info.Khac.forEach(function(khac,i){ %>
			<h1 class="text">     <%=khac%> </h1>>
		  	<% }); %>	
	</div>






</div>
<!-- MainBody End ============================= -->
<!-- Footer ================================================================== -->
	<%include footer%>
<!-- Placed at the end of the document so the pages load faster ============================================= -->
	<script src="/customer/themes/js/jquery.js" type="text/javascript"></script>
	<script src="/customer/themes/js/bootstrap.min.js" type="text/javascript"></script>
	<script src="/customer/themes/js/google-code-prettify/prettify.js"></script>
	
	<script src="/customer/themes/js/bootshop.js"></script>
    <script src="/customer/themes/js/jquery.lightbox-0.5.js"></script>
	
</body>
</html>